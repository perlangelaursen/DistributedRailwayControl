<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension
      point="org.eclipse.ui.menus">
    <menuContribution
        locationURI="popup:org.eclipse.ui.popup.any?after=additions">
      <menu
          id="s144449s144456.RailwayModelGenerator"
          label="Generate Railway Control System Model">
      </menu>
    </menuContribution>
  </extension>

  <extension
      point="org.eclipse.ui.menus">
    <menuContribution
        locationURI="popup:s144449s144456.RailwayModelGenerator?after=additions">
      <command
          commandId="s144449s144456.RailwayModelGenerator.generateCancelUppaal">
        <visibleWhen>
          <with
              variable="selection">
            <count value="1"/>
            <iterate>
              <adapt
                  type="network.Network">
              </adapt>
            </iterate>
          </with>
        </visibleWhen>
      </command>
      <command
          commandId="s144449s144456.RailwayModelGenerator.generateGeneralUppaal">
        <visibleWhen>
          <with
              variable="selection">
            <count value="1"/>
            <iterate>
              <adapt
                  type="network.Network">
              </adapt>
            </iterate>
          </with>
        </visibleWhen>
      </command>
      <command
          commandId="s144449s144456.RailwayModelGenerator.generateGlobalUppaal">
        <visibleWhen>
          <with
              variable="selection">
            <count value="1"/>
            <iterate>
              <adapt
                  type="network.Network">
              </adapt>
            </iterate>
          </with>
        </visibleWhen>
      </command>
      <command
          commandId="s144449s144456.RailwayModelGenerator.generateStrictUppaal">
        <visibleWhen>
          <with
              variable="selection">
            <count value="1"/>
            <iterate>
              <adapt
                  type="network.Network">
              </adapt>
            </iterate>
          </with>
        </visibleWhen>
      </command>
      <command
          commandId="s144449s144456.RailwayModelGenerator.generateUMC">
        <visibleWhen>
          <with
              variable="selection">
            <count value="1"/>
            <iterate>
              <adapt
                  type="network.Network">
              </adapt>
            </iterate>
          </with>
        </visibleWhen>
      </command>
    </menuContribution>
  </extension>

  <extension
      point="org.eclipse.ui.commands">
    <command
        id="s144449s144456.RailwayModelGenerator.generateCancelUppaal"
        description="Generate Uppaal Code with Cancel operation"
        defaultHandler="s144449s144456.RailwayModelGenerator.commands.UppaalCancelTranslator"
        name="Generate Uppaal Code (Extension with cancel operation)">
    </command>
  </extension>

  <extension
      point="org.eclipse.ui.commands">
    <command
        id="s144449s144456.RailwayModelGenerator.generateGeneralUppaal"
        description="Generate Uppaal Code (Standard version)"
        defaultHandler="s144449s144456.RailwayModelGenerator.commands.UppaalGeneralTranslator"
        name="Generate Uppaal Code (Standard version)">
    </command>
  </extension>

  <extension
      point="org.eclipse.ui.commands">
    <command
        id="s144449s144456.RailwayModelGenerator.generateGlobalUppaal"
        description="Generate Uppaal Code (Global memory access)"
        defaultHandler="s144449s144456.RailwayModelGenerator.commands.UppaalGlobalTranslator"
        name="Generate Uppaal Code (Global memory access)">
    </command>
  </extension>

  <extension
      point="org.eclipse.ui.commands">
    <command
        id="s144449s144456.RailwayModelGenerator.generateStrictUppaal"
        description="Generate Uppaal Code (Strict evaluation sequence)"
        defaultHandler="s144449s144456.RailwayModelGenerator.commands.UppaalStrictTranslator"
        name="Generate Uppaal Code (Strict evaluation sequence)">
    </command>
  </extension>

  <extension
      point="org.eclipse.ui.commands">
    <command
        id="s144449s144456.RailwayModelGenerator.generateUMC"
        description="Generate UMC Code"
        defaultHandler="s144449s144456.RailwayModelGenerator.commands.UMCTranslator"
        name="Generate UMC Code">
    </command>
  </extension>
</plugin>
